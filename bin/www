var app = require('../dist/server');
var port = process.env.PIRATEJAM_PORT || 8888
app.listen(port, function() {
    console.log('__________________________________________________')
    console.log('');
    console.log(' Server now Running at localhost:' + port);
    console.log('');
    console.log('');
    console.log('Version 0.1                     Michael Harrison');
    console.log('');
    console.log('__________________________________________________')
    console.log('\n\n');
});